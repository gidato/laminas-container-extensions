{
    "name": "gidato/laminas-container-extensions",
    "type": "library",
    "description": "Extensions Laminas Service Manager to implement Container Extensions (eg nested containers, cached instances with options)",
    "homepage": "https://github.com/gidato/laminas-container-extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "GIDATO",
            "homepage": "http://www.gidato.com/"
        }
    ],
    "require": {
        "gidato/container-extensions": "^1.1",
        "laminas/laminas-servicemanager": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },

    "autoload": {
        "psr-4": {
            "Gidato\\ContainerExtensions\\Laminas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
